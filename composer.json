{
	"name": "josephscott/phutility",
	"description": "PHP utilities",
	"license": "MIT",
	"require": {
		"php": ">=8.2.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.10",
		"pestphp/pest": "^2.18",
		"friendsofphp/php-cs-fixer": "^3.34"
	},
	"autoload": {
		"classmap": [
			"src/numberseries.php"
		]
	},
	"config": {
		"classmap-authoritative": true,
		"allow-plugins": {
			"pestphp/pest-plugin": false
		}
	}
}
